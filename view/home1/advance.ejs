<%- include('../_partial/_header'); -%>
<!--contact us start-->
<section>
    <div class="container mb-4">
        <h1 class="text-center text-capitalize pt-3">Register</h1>
        <hr class="w-25 mx-auto pb-5">  
        <div class="w-50 mx-auto">
            <form action="/advance" method="POST">
            <div class="form-group">
              <label for="Name">Full Name:</label>
              <input type="text" name="username" class="form-control" placeholder="Enter Name" id="name" autocomplete="off">
            </div>
            <div class="form-group">
              <label for="password">Password:</label>
              <input type="password" name="password" class="form-control" placeholder="Password" id="password">
            </div>
            <input type="submit"class="btn btn-primary">
          </form>       
         </div>
</section>
<%- include ('../_partial/_footer'); -%>